{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "css": "css",
    "scss": "scss"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class:\\s*?[\"'`]([^\"'`]*).*?[\"'`]", "([a-zA-Z0-9\\-:]+)"],
    ["className\\s*:\\s*[\"'`]([^\"'`]*).*?[\"'`]", "([a-zA-Z0-9\\-:]+)"]
  ]
}